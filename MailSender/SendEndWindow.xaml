<Window x:Class="MailSender.SendEndWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MailSender"
        mc:Ignorable="d"
        Title="SendEndWindow" Height="150" Width="400" HorizontalAlignment="Center" VerticalAlignment="Center" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" WindowStyle="None" BorderThickness="1" BorderBrush="#FFB4B4B4">
    <Grid>
        <Label x:Name="lSendEnd" Content="Работа завершена" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" FontSize="36"/>
        <Button x:Name="btnOk" Content="Ok" HorizontalAlignment="Center" Margin="0,0,0,16" VerticalAlignment="Bottom" Width="128" Click="BtnOk_Click"/>

    </Grid>
</Window>
